def is_sum(lst: list, num: int) -> bool:
    original_len = len(lst)
    for k in range(original_len - 1):
        for i in range(1, len(lst)):
            sum = lst[0] + lst[i]
            if sum == num:
                return True
        
        lst.pop(0)
        
        
        
    return False
    


